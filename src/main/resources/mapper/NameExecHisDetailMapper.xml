<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hand.hdsp.quality.infra.mapper.NameExecHisDetailMapper">
	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap id="BaseResultMap" type="com.hand.hdsp.quality.domain.entity.NameExecHisDetail">
        <result column="detail_id" property="detailId" jdbcType="DECIMAL"/>
        <result column="history_id" property="historyId" jdbcType="DECIMAL"/>
        <result column="schema_name" property="schemaName" jdbcType="VARCHAR"/>
        <result column="table_name" property="tableName" jdbcType="VARCHAR"/>
        <result column="error_message" property="errorMessage" jdbcType="VARCHAR"/>
        <result column="source_path" property="sourcePath" jdbcType="VARCHAR"/>
        <result column="tenant_id" property="tenantId" jdbcType="DECIMAL"/>
        <result column="object_version_number" property="objectVersionNumber" jdbcType="DECIMAL"/>
        <result column="creation_date" property="creationDate" jdbcType="TIMESTAMP"/>
        <result column="created_by" property="createdBy" jdbcType="DECIMAL"/>
        <result column="last_updated_by" property="lastUpdatedBy" jdbcType="DECIMAL"/>
        <result column="last_update_date" property="lastUpdateDate" jdbcType="TIMESTAMP"/>
        <result column="project_id" property="projectId" jdbcType="DECIMAL"/>
    </resultMap>
    <resultMap id="DtoResultMap" type="com.hand.hdsp.quality.api.dto.NameExecHisDetailDTO" extends="BaseResultMap">

    </resultMap>
    <sql id="BaseProperties">
        ${prefix}.detail_id,
        ${prefix}.history_id,
        ${prefix}.schema_name,
        ${prefix}.error_message,
        ${prefix}.source_path,
        ${prefix}.tenant_id,
        ${prefix}.object_version_number
    </sql>
    <select id="list" resultType="com.hand.hdsp.quality.api.dto.NameExecHisDetailDTO">
        SELECT
        <include refid="BaseProperties">
            <property name="prefix" value="xnehd"/>
        </include>
        FROM xsta_name_exec_his_detail xnehd
        WHERE xnehd.history_id=#{historyId}
    </select>
</mapper>